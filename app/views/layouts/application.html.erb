<!DOCTYPE html>
<html>
  <head>
    <title>ApportmentScheduler</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% flash.each do |type, msg| %>
      <div>
        <%= msg %>
      </div>
    <% end %>
    <% if user_signed_in? %>
      <%= button_to "logout", destroy_user_session_path, method: :delete  %>
    <% end %>
    <%= yield %>
  </body>
</html>
